    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch LLDB",
            "program": "${workspaceFolder}/build/index",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch GDB",
            "target": "./build/index",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/build"
        }

    ]
}